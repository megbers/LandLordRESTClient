<form role="form">
<h2>New Property</h2>
<div class="form-group">
	<label for='address'>Address</label> <br/>
	<input type='text' class="form-control" name='address' id='address'/>
</div>
<div class="form-group">
    <label for='mortgage'>Mortgage Amount</label> <br/>
    <input type='number' class="form-control" name='mortgage' id='mortgage'/>
</div>
<div class="form-group">
    <label for='currentRent'>Current Rent</label> <br/>
    <input type='number' class="form-control" name='currentRent' id='currentRent'/>
</div>
<div class="form-group">
    <label for='tax'>Tax</label> <br/>
    <input type='number' class="form-control" name='tax' id='tax'/>
</div>
<div class="form-group">
    <label for='escrow'>Escrow</label> <br/>
    <input type='text' class="form-control" name='escrow' id='escrow'/>
</div>
<div class="form-group">
    <label for='security'>Security Deposite</label> <br/>
    <input type='number' class="form-control" name='security' id='security'/>
</div>
<div class="form-group">
    <label for='pet'>Pet Deposite</label> <br/>
    <input type='number' class="form-control" name='pet' id='pet'/>
</div>
<div class="form-group">
    <label for='rentPermit'>Rent Permit Date</label> <br/>
    <input type='text' class="form-control" name='rentPermit' id='rentPermit'/>
</div>
<div class="form-group">
    <label for='leaseStart'>Lease Start Date</label> <br/>
    <input type='text' class="form-control" name='leaseStart' id='leaseStart'/>
</div>
<div class="form-group">
    <label for='leaseEnd'>Lease End Date</label> <br/>
    <input type='text' class="form-control" name='leaseEnd' id='leaseEnd'/>
</div>
<div class="form-group">
    <label for='imageLocation'>Image Location</label> <br/>
    <input type='url' class="form-control" name='imageLocation' id='imageLocation'/>
</div>


<div class="form-group">
    <label for='tenant'>Tenant</label> <br/>
    <select class="form-control" name='tenant' id='tenant'>
        <option></option>
        <%for(var i = 0; i < this.length ; i++){%>
        <option value="<%= this[i].id%>"><%= this[i].name%></option>
        <%}%>
    </select>
</div>

<div class="form-group">
    <input type="submit" class="btn btn-primary btn-lg btn-block" value='Create'/>
    <button type="button" class="cancelButton btn btn-primary btn-lg btn-block">Cancel</button>
    <!-- input type='submit' value='Create'/ -->
</div>
</form>