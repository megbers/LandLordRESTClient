<div class="personTable">
    <table class="table table-striped" id='persons'>
        <tr>
            <th>Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Type</th>
        </tr>
        <%for(var i = 0; i < this.length ; i++){ %>
        <tr <%= this[i]%>>
            <%== $.View('//landlord/person/list/views/person.ejs', this[i] )%>
        </tr>
        <%}%>
    </table>
    <a id='createPersonButton'><button type="button" class="importExportButton btn btn-primary btn-lg btn-block">Create Person</button></a>
</div>
