<form role="form">
    <div class="form-group">
        <label for='resetYear'>Reset Data</label> <br/>
        <select id="resetYear" class="form-control">
            <option value="2009">2009</option>
            <option value="2010">2010</option>
            <option value="2011">2011</option>
            <option value="2012">2012</option>
            <option value="2013">2013</option>
            <option value="2014">2014</option>
            <option value="2015">2015</option>
            <option value="2016">2016</option>
            <option value="2017">2017</option>
            <option value="2018">2018</option>
        </select>
        <select id="resetMonth" class="form-control">
            <option value="0">January</option>
            <option value="1">February</option>
            <option value="2">March</option>
            <option value="3">April</option>
            <option value="4">May</option>
            <option value="5">June</option>
            <option value="6">July</option>
            <option value="7">August</option>
            <option value="8">September</option>
            <option value="9">October</option>
            <option value="10">November</option>
            <option value="11">December</option>
        </select>
    </div>
    <div class="form-group">
        <a class="resetDataButton"><button type="button" class="buttonsInList btn btn-primary btn-lg btn-block">Reset Data</button></a>
        <a class="resetAllButton"><button type="button" class="buttonsInList btn btn-primary btn-lg btn-block">Reset All Data</button></a>
    </div>
    <div class="form-group">
        <label for='vacancyRate'>Vacancy Rate</label> <br/>
        <input id="vacancyRate" class="form-control" value=""/>
    </div>
    <div class="form-group">
        <label for='advertiserWebsite'>Advertiser Website</label> <br/>
        <input id="advertiserWebsite" class="form-control" value=""/>
    </div>
    <div class="form-group">
        <a class="importExportButton"><button type="button" class="buttonsInList btn btn-primary btn-lg btn-block">Import / Export</button></a>
        <a class="reportButton"><button type="button" class="buttonsInList btn btn-primary btn-lg btn-block">Reports</button></a>
        <a class="showTutorialButton"><button type="button" class="buttonsInList btn btn-primary btn-lg btn-block">Show Tutorial</button></a>
    </div>
</form>